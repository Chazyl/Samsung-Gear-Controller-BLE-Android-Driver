<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- Center circle -->
    <path
        android:fillColor="#CCCCCC"
        android:pathData="M50,50 m-10,0 a10,10 0 1,0 20,0 a10,10 0 1,0 -20,0" />

    <!-- Up arrow -->
    <path
        android:fillColor="#CCCCCC"
        android:pathData="M40,50 L50,20 L60,50 Z"
        android:name="up" />

    <!-- Right arrow -->
    <path
        android:fillColor="#CCCCCC"
        android:pathData="M50,40 L80,50 L50,60 Z"
        android:name="right" />

    <!-- Down arrow -->
    <path
        android:fillColor="#CCCCCC"
        android:pathData="M40,50 L50,80 L60,50 Z"
        android:name="down" />

    <!-- Left arrow -->
    <path
        android:fillColor="#CCCCCC"
        android:pathData="M50,40 L20,50 L50,60 Z"
        android:name="left" />
</vector>
